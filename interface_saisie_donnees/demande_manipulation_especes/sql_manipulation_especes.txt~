UPDATE demande_manipulation_especes.sites_amsterdam SET toponyme_site=(select split_part(sites_amsterdam.nom_site,',',1));

UPDATE demande_manipulation_especes.sites_amsterdam SET toponyme_type=(select substring(split_part(sites_amsterdam.nom_site,',',2) from $$^\s*\S+\S+$$));

UPDATE demande_manipulation_especes.sites_amsterdam SET toponyme_type=replace(toponyme_type, ' ', '');



UPDATE demande_manipulation_especes.sites_crozet SET toponyme_site=(select split_part(sites_crozet.nom_site,',',1));

UPDATE demande_manipulation_especes.sites_crozet SET toponyme_type=(select substring(split_part(sites_crozet.nom_site,',',2) from $$^\s*\S+\S+$$));

UPDATE demande_manipulation_especes.sites_crozet SET toponyme_type=replace(toponyme_type, ' ', '');


UPDATE demande_manipulation_especes.sites_kerguelen SET toponyme_site=(select split_part(sites_kerguelen.nom_site,',',1));

UPDATE demande_manipulation_especes.sites_kerguelen SET toponyme_type=(select substring(split_part(sites_kerguelen.nom_site,',',2) from $$^\s*\S+\S+$$));

UPDATE demande_manipulation_especes.sites_kerguelen SET toponyme_type=replace(toponyme_type, ' ', '');
