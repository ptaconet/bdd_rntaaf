DROP SCHEMA IF EXISTS demande_manipulation_especes CASCADE;

CREATE SCHEMA demande_manipulation_especes
AUTHORIZATION postgres;



CREATE TABLE demande_manipulation_especes.demandeur (
nom VARCHAR(50) PRIMARY KEY,
prenom VARCHAR(50),
organisme_rattachement VARCHAR(100),
adresse VARCHAR(200),
telephone VARCHAR(20),
mail VARCHAR(50),
mot_de_passe VARCHAR(15)
);

CREATE TABLE demande_manipulation_especes.programme (
--pk_programme SERIAL PRIMARY KEY,
titre TEXT,
numero SMALLINT PRIMARY KEY,
fk_responsable VARCHAR(50),
FOREIGN KEY (fk_responsable) REFERENCES demande_manipulation_especes.demandeur(nom) ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE demande_manipulation_especes.sites_kerguelen (
nom_site VARCHAR(100) PRIMARY KEY,
toponyme_site VARCHAR(100),
toponyme_type VARCHAR(100),
demande_pour_acces BOOLEAN,    
statut_protection VARCHAR(20),
latitude INTEGER,
longitude INTEGER,
the_geom geometry(Point,32742)
);

CREATE TABLE demande_manipulation_especes.sites_crozet (
nom_site VARCHAR(100) PRIMARY KEY,
toponyme_site VARCHAR(100),
toponyme_type VARCHAR(100),
demande_pour_acces BOOLEAN, 
statut_protection VARCHAR(20),
latitude INTEGER,
longitude INTEGER,
the_geom geometry(Point,32739)
);

CREATE TABLE demande_manipulation_especes.sites_amsterdam (
nom_site VARCHAR(100) PRIMARY KEY,
toponyme_site VARCHAR(100),
toponyme_type VARCHAR(100),
demande_pour_acces BOOLEAN, 
statut_protection VARCHAR(20),
latitude INTEGER,
longitude INTEGER,
the_geom geometry(Point,32743)
);

CREATE TABLE demande_manipulation_especes.especes (
nom_commun VARCHAR(100) PRIMARY KEY,
nom_scientifique VARCHAR(100),
famille VARCHAR(50),
ordre VARCHAR(50),
classe VARCHAR(20),
protection_nationale BOOLEAN,
);


CREATE TABLE demande_manipulation_especes.demande (
numero_demande SERIAL PRIMARY KEY,
date_demande DATE,
saison CHAR(9), -- ex: 2012/2013
fk_programme INTEGER,
district VARCHAR(15),
site VARCHAR(100),
date_debut DATE,
date_fin DATE,

finalite_inventaire BOOLEAN,
finalite_suivi_pop BOOLEAN,
finalite_etude_ecoethologique BOOLEAN,
finalite_etude_parasitologique BOOLEAN,
finalite_etude_genetique BOOLEAN,
finalite_etude_biometrique BOOLEAN,
finalite_etude_physiologique BOOLEAN,
finalite_etude_epidemiologique BOOLEAN,
finalite_autre TEXT, --NULL ou un texte

type_capture_euthanasie BOOLEAN,
type_capture_definitive BOOLEAN,
type_capture_temporaire_relachediffere BOOLEAN,
type_capture_temporaire_relacheimmediat BOOLEAN,

mode_capture_manuelle BOOLEAN,
mode_capture_aufilet BOOLEAN,
mode_capture_pieges BOOLEAN,
mode_capture_autre TEXT,

marquage BOOLEAN,
marquage_fishtag BOOLEAN,
marquage_baguesplastiques BOOLEAN,
marquage_porcimark BOOLEAN,
marquage_nyanzol BOOLEAN,
marquage_leger_autre TEXT,
marquage_baguemetal BOOLEAN,
marquage_aileron BOOLEAN,
marquage_transpondeur BOOLEAN,
marquage_definitif_autre TEXT,
marquage_autre TEXT,

equipement BOOLEAN,
equipement_gps BOOLEAN,
equipement_gls BOOLEAN,
equipement_vhf BOOLEAN,
equipement_argos BOOLEAN,
equipement_externe_autre TEXT,
equipement_interne_sondeoesophagienne BOOLEAN,
equipement_interne_autre TEXT,

prelevement BOOLEAN,
prelevement_lavagestomacal BOOLEAN,
prelevement_prisedesang BOOLEAN,
prelevement_muscles BOOLEAN,
prelevement_autres TEXT,
prelevement_interventionchirugicale BOOLEAN,
prelevement_type_interventionchirugicale TEXT,

commentaires TEXT,
piece_jointe BOOLEAN,

FOREIGN KEY (fk_programme) REFERENCES demande_manipulation_especes.programme(numero) ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE demande_manipulation_especes.espece_demande (
pk_espece_demande SERIAL PRIMARY KEY,
fk_numero_demande INTEGER,
fk_espece_manipulee VARCHAR(100),
nb_individus_adultes_males SMALLINT,
nb_individus_adultes_femelles SMALLINT,
nb_individus_adultes_indetermines SMALLINT,
nb_individus_juveniles_males SMALLINT,
nb_individus_juveniles_femelles SMALLINT,
nb_individus_juveniles_indetermines SMALLINT,
nb_individus_indetermines SMALLINT,
nb_individus_oeufs SMALLINT,
nb_individus_total SMALLINT,
FOREIGN KEY (fk_numero_demande) REFERENCES demande_manipulation_especes.demande(numero_demande) ON UPDATE CASCADE ON DELETE CASCADE,
FOREIGN KEY (fk_espece_manipulee) REFERENCES demande_manipulation_especes.especes(nom_commun) ON UPDATE CASCADE ON DELETE CASCADE
);



CREATE TABLE demande_manipulation_especes.autorisation_demande (
fk_numero_demande INTEGER PRIMARY KEY,
avis_cep VARCHAR(15),
recommandations_cep TEXT,
avis_cnpn VARCHAR(15),
recommandations_cnpn TEXT,
decision_adm_superieur VARCHAR(15),
duree INTEGER,
FOREIGN KEY (fk_numero_demande) REFERENCES demande_manipulation_especes.demande(numero_demande) ON UPDATE CASCADE ON DELETE CASCADE
);


